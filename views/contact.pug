extends base.pug
block scripts 
    script(src='../static/script.js')
block styles
    style
      include ../static/style.css
      include ../static/contactCSS.css
block content 
    h1 Contact Us
    form(action="/contact" ,method="post" ,class="formgrp")
        input(type="text" name="name" placeholder="Enter your Name" id="name")
        input(type="email" name="email" placeholder="Enter your Email ID" id="email")
        input(type="text" name="gender" placeholder="Enter your Gender" id="gender")
        input(type="text" name="address" placeholder="Enter your Address" id="address")
        input(type="text" name="more" placeholder="More about You" id="more")
        button(class="btn")
            | Submit